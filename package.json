{
  "name": "lottie-icons",
  "version": "1.1.3",
  "description": "Beautiful animated React icons powered by Lottie - Lucide-style API with SSR support",
  "main": "./dist/lottie-icons.es.js",
  "module": "./dist/lottie-icons.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/lottie-icons.es.js"
    },
    "./icons/Heart": {
      "types": "./dist/icons/Heart.d.ts",
      "import": "./dist/icons/Heart.js"
    },
    "./icons/Check": {
      "types": "./dist/icons/Check.d.ts",
      "import": "./dist/icons/Check.js"
    },
    "./icons/Loader": {
      "types": "./dist/icons/Loader.d.ts",
      "import": "./dist/icons/Loader.js"
    },
    "./icons/ArrowRight": {
      "types": "./dist/icons/ArrowRight.d.ts",
      "import": "./dist/icons/ArrowRight.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "USAGE_EXAMPLES.md"
  ],
  "scripts": {
    "dev": "vite",
    "build:lib": "tsc && vite build --mode lib",
    "build": "vite build",
    "preview": "vite preview",
    "prepublishOnly": "pnpm build:lib"
  },
  "keywords": [
    "react",
    "icons",
    "lottie",
    "animation",
    "animated-icons",
    "lucide",
    "svg",
    "components",
    "react-icons",
    "lottie-web",
    "nextjs",
    "ssr",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/frostfoe/lottie-icons.git"
  },
  "bugs": {
    "url": "https://github.com/frostfoe/lottie-icons/issues"
  },
  "homepage": "https://github.com/frostfoe/lottie-icons#readme",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "lottie-web": "^5.13.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4"
  }
}
